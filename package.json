{
  "name": "library-vs-framework",
  "version": "1.0.0",
  "description": "Starter project for kata (Node, TypeScript, Jest)",
  "main": "index.js",
  "scripts": {
    "execute": "npx ts-node -r tsconfig-paths/register",
    "library": "npm run execute -- src/demo/library.ts",
    "framework": "npm run execute -- src/demo/framework.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieueveillard/library-vs-framework.git"
  },
  "keywords": [
    "starter",
    "project",
    "kata",
    "node",
    "typescript"
  ],
